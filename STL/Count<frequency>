#include <iostream>
#include <map>

int main() {
    int n;
    std::cin >> n;
    int arr[n];

    // Input array elements
    for (int i = 0; i < n; i++) {
        std::cin >> arr[i];
    }

    std::map<int, int> frequency;

    // Count the frequency of each element using a map
    for (int i = 0; i < n; i++) {
        frequency[arr[i]]++;
    }

    // Output the frequencies
    for (const auto &pair : frequency) {
        std::cout << pair.first << ": " << pair.second << std::endl;
    }

    return 0;
}

