#include <iostream>
#include <map>
#include <vector>

int main() {
    int n;
    std::cin >> n;

    std::vector<int> elements(n);

    for (int i = 0; i < n; i++) {
        std::cin >> elements[i];
    }

    std::map<int, int> elementIndexMap;

    for (int i = 0; i < n; i++) {
        if (elementIndexMap.find(elements[i]) != elementIndexMap.end()) {
            // Found a duplicate element
            std::cout << elements[i] << " is the first duplicate element at index " << elementIndexMap[elements[i]] << std::endl;
            break;
        }
        elementIndexMap[elements[i]] = i;
    }

    return 0;
}
