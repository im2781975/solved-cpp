//use of std::swap_ranges 
#include <iostream> 
#include <algorithm> 
#include <vector> 
using namespace std; 
int main() 
{ 
    vector<int> v1 = { 1, 2, 3, 4, 5, 1, 2, 3, 4 }; 
    int i; 
    
    vector<int> v2 = v1; 
 
    vector<int> v3 = { 1, 2, 3, 4, 1, 2, 3, 5 }; 

    vector<int> v4 = v3; 
  
    if (v1.size() % 2 == 0) 
        swap_ranges(v1.begin(), v1.begin() + (v1.size() / 2), v1.begin() + v1.size() / 2); 
  
    else
        swap_ranges(v1.begin(), v1.begin() + v1.size() / 2, v1.begin() + (v1.size() / 2) + 1); 
  
    if (v1 == v2)  
    { 
        cout << "Yes"; 
    } else 
    { 
        cout << "No"; 
    } 

    if (v3.size() % 2 == 0) 
        swap_ranges(v3.begin(), v3.begin() + (v3.size() / 2), v3.begin() + v3.size() / 2); 
  
    else
        swap_ranges(v3.begin(), v3.begin() + v3.size() / 2,  v3.begin() + (v3.size() / 2) + 1); 
  
    cout << "\n"; 
    if (v3 == v4)  
    { 
        cout << "Yes"; 
    } else 
    { 
        cout << "No"; 
    } 
    return 0; 
} 
