 /*returns the elements in a vector
 that are even numbered */
#include<bits/stdc++.h>
using namespace std;
vector<int> even_generator(vector<int> &ary)
{
    vector<int> even;
    for(int i=0;i<ary.size();i++)
    {
        if(ary[i]%2==0)
        even.push_back(ary[i]);
    }
    return even;
}
int main ()
{
    vector<int> ary = {1,2,3,4,5,6,7,8,9};
    vector<int> even = even_generator(ary);
    for(int i=0;i<even.size();i++)
    {
        cout<< even[i]<<" ";
    }
}
