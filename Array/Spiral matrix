#include<bits/stdc++.h>
using namespace std;
int main()
{
    int n;
    cin>>n;
    int arr[n+2][n+2];
    for(int i=0;i<n;i++)
    {
        for(int j=0;j<n;j++)
        {
            cin>>arr[i][j];
        }
    }
    for(int i=0; i<n; i++)
    {
        for(int j=0; j<n; j++)
        {
            if(i==0 && j!=n-1)
            {
                cout<<arr[i][j]<<" ";
            }
            
            if(j==n-1 && i!=n-1)
            {
                cout<<arr[i][j]<<" ";
            }
        }
    }
    for(int i=0; i<n; i++)
    {
        for(int j=n-1; j>=0; j--)
        {
            if(i==n-1)
            {
            cout<<arr[i][j]<<" ";
            }
        }
    }
    for(int i=0; i<n; i++)
    {
        for(int j=0; j<n; j++)
        {
            if(i==n-2)
            {
            cout<<arr[i][j]<<" ";
            }
        }
    }
}
