// std::partial_sort_copy 
#include <iostream> 
#include <algorithm> 
#include <vector> 
using namespace std; 
  
bool comp(int a, int b) 
{ 
    return (a < b); 
} 
int main() 
{ 
    vector<int> v = { 1, 1, 3, 10, 3, 3, 7, 7, 8 }, v1(7); 
  
    vector<int>::iterator ip; 
  
    partial_sort_copy(v.begin(), v.end(), v1.begin(),v1.end(),comp); 
  
    for (ip = v1.begin(); ip != v1.end(); ++ip) { 
        cout << *ip << " "; 
    } 
    return 0; 
} 
