// std::partial_sort
#include <iostream>
#include <algorithm>
#include <vector>
using namespace std;
int main()
{
    vector<int> v = { 10, 45, 60, 78, 23, 21, 3 }, v1;
 
    v1 = v;
    vector<int>::iterator ip;
     
    
    partial_sort(v.begin(), v.begin() + 2, v.end());
 
    sort(v1.begin(), v1.begin()+2);
 
    cout << "\nAfter partial sort() vect-v is : ";
    for (int i = 0; i < 2; ++i) {
        cout << v[i] << " ";
    }
 
    cout << "\nAfter sort in range vect-v1 is : ";
    for (int i = 0; i < 2; ++i) {
        cout << v1[i] << " ";
    }
    return 0;
}
