// working of merge() and include()
#include<iostream>
#include<algorithm>
#include<vector>
using namespace std;
int main()
{
    vector<int> v1 = {1, 3, 4, 5, 20, 30};

    vector<int> v2 = {1, 5, 6, 7, 25, 30};
    
    vector<int> v3(12);
     
    merge(v1.begin(), v1.end(), v2.begin(), v2.end(), v3.begin());
     
    cout << "\nThe new container after merging is : ";
    for (int &x : v3) 
     cout << x << " ";
 
    vector<int> v4 = {1, 3, 4, 5, 6, 20, 25, 30};
  
   includes(v4.begin(), v4.end(), v1.begin(), v1.end())?
        cout << "\nv4 includes v1":
        cout << "\nv4 does'nt include v1";
  
 return 0;    
}
