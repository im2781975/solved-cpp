 // std::partial_sort
#include <iostream>
#include <vector>
#include <algorithm>
using namespace std;
int main()
{
    vector<int> v = { 1, 3, 1, 10, 3, 3, 7, 7, 8 }, i;
 
    vector<int>::iterator ip;
    partial_sort(v.begin(), v.begin() + 3, v.end());

    for (ip = v.begin(); ip != v.end(); ++ip) {
        cout << *ip << " ";
    }
    return 0;
}
