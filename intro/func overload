#include <bits/stdc++.h>
using namespace std;
class Test {
    protected:
    int x;
 
    public:
    Test(int i): x(i)
    {
    }
    void fun() const
    {
        cout << "fun() const called " << "\n";
    }
    void fun() { cout << "fun() called " << "\n"; }
};
int main()
{
    Test t1(10);
    const Test t2(20);
    t1.fun();
    t2.fun();
    return 0;
}
