#include<bits/stdc++.h>
using namespace std;
int n=5;
void print_1()
{
    vector<int>v(5,3);
    for(int i=0;i<5-2;i++)
        v[i]=i;
        
    for(int i=0;i<5;i++)
        cout<<v[i]<<" ";
}
void print_2()
{
    vector<int>v(5,0);
    v[1]=5;
    
    cout<<v.front()<<" ";
}
void print_3()
{
    vector<int>v(5,0);
    v.resize(10,10);
    
    cout<<v[7]<<" ";
}
void print_4()
{
    vector<int>v[1];
    v[0].push_back(9);
    v[0].push_back(2);
    vector<int>t=v[0];
    
    cout<<t[0];
}

void print_5()
{
    vector<int>a(2);
     a[2-1]=2;
     cout<<a[0];
}
void print_6()
{
    int i,j;
    vector<int>v[n+1];
    for( i=1;i<=n;i++)
    {
    for(j=1;j<=n;j++)
    v[i].push_back(j);
    }
}
void print_7()
{
    vector<int>v(n*n);
    for(int i=0;i<n*n;i++)
    v[i]=i;
    
    int sum=0;
    for(int i=0;i<n*n;i++)
    sum+=v[i];
    
    cout<<sum;
}
void print_8()
{
    vector<int>v;
    for(int i=0;i<n;i++)
    v.push_back(i);
    for(int i=0;i<n;i++)
    cout<<v[i]<<" ";
}

int main()
{
    print_1();
    cout<<"\n";
    print_2();
    cout<<"\n";
    print_3();
    cout<<"\n";
    print_4();
    cout<<"\n";
    print_5();
    cout<<"\n";
    print_6();
    cout<<"\n";
    print_7();
    cout<<"\n";
    print_8();
    cout<<"\n";
}

